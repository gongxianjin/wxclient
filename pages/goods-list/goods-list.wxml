<view class="container-box">
  <view class='img-box'>
    <canvas style="width:686rpx;height:686rpx;background:#f1f1f1;" canvas-id="mycanvas"></canvas>
  </view>
  <form>
    <view class='input-row'>
      <label>网址</label>
      <input name="url" type='text' maxlength='25' placeholder="{{placeholder}}"></input>
